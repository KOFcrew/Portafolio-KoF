    <!-- Texto -->
    <section class="text-center md:text-left">
      <h2 class="text-3xl font-bold">
        <span class="text-teal-600">Páginas Web</span>
      </h2>
      <p class="mt-4 text-gray-700 leading-relaxed">
        Nuestros proyectos de páginas web creadas con las habilidades presentadas.
